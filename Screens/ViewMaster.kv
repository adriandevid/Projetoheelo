<WindowOfLogin>:
    BoxLayout:
        orientation: "vertical"
        padding: "5dp"
        size_hint: None, None
        size: 300, 500
        elevation: 30
        radius: [70]
        pos_hint: {'center_x': 0.5, 'center_y': 0.3}
        canvas:
            Color:
                rgba: app.theme_cls.primary_color
            Ellipse:
                size: 150, 150
                pos: self.x + 75, self.y + 400
                source: 'C:/Users/PC/Documents/ProjetoHeelo/Screens/fotos/do-utilizador.png'
        FloatLayout:
            MDTextField:
                hint_text: "E-mail"
                helper_text: "Digite o e-mail comforme o cadastrado ex:. exemplo@exemplo.com"
                helper_text_mode: "on_focus"
                color_mode: "custom"
                fill_color: 0,0,0,1
                pos_hint: {'center_x': 0.5, 'center_y': 0.6}
            MDTextField:
                hint_text: "Password"
                helper_text: ""
                password: True
                helper_text_mode: "on_focus"
                pos_hint: {'center_x': 0.5, 'center_y': 0.482}
            MDLabel:
                text: "Esqueçeu a senha? click"
                theme_text_color: "Custom"
                text_color: 0.12,0.54,0.84,0.5
                font_size: 11
                pos_hint: {'center_x': 0.5, 'center_y': 0.4}
            MDTextButton:
                text: "aqui"
                custom_color: 0.12,0.54,0.84,0.9
                pos_hint: {'center_x': 0.5, 'center_y': 0.4}
            MDRoundFlatIconButton:
                text: '                        Entrar'
                icon: 'account-check'
                size_hint_x: .9
                pos_hint: {'center_x': 0.5, 'center_y': 0.3}
                on_release: app.root.current = 'WindowQuiz'
            MDFillRoundFlatIconButton:
                text: 'cadastrar'
                icon: 'account-plus'
                size_hint_x: .9
                md_bg_color: app.theme_cls.primary_color
                pos_hint: {'center_x': 0.5, 'center_y': 0.2}
                on_release: app.root.current = 'WindowQuiz'
