            orientation: "vertical"
            padding: "3dp"
            size_hint: 1, 1
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
            AsyncImage:
                source: 'C:/Users/PC/Documents/ProjetoHeelo/Screens/fotos/pandemia.png'
                size_hint: 1,1
                pos_hint:{'center_x': 0.5, 'center_y': 0.5}
