<WindowOfLogin>:
    BoxLayout:
        orientation: "vertical"
        padding: "5dp"
        size_hint: None, None
        size: 300, 500
        elevation: 30
        radius: [70]
        pos_hint: {'center_x': 0.5, 'center_y': 0.3}
        canvas:
            Color:
                rgba: app.theme_cls.primary_color
            Ellipse:
                size: 150, 150
                pos: self.x + 75, self.y + 400
                source: 'C:/Users/PC/Documents/ProjetoHeelo/Screens/fotos/do-utilizador.png'
        FloatLayout:
            MDTextField:
                hint_text: "E-mail"
                helper_text: "Digite o e-mail comforme o cadastrado ex:. exemplo@exemplo.com"
                helper_text_mode: "on_focus"
                color_mode: "custom"
                fill_color: 0,0,0,1
                pos_hint: {'center_x': 0.5, 'center_y': 0.6}
            MDTextField:
                hint_text: "Password"
                helper_text: ""
                password: True
                helper_text_mode: "on_focus"
                pos_hint: {'center_x': 0.5, 'center_y': 0.482}
            MDLabel:
                text: "Esqueçeu a senha? click"
                theme_text_color: "Custom"
                text_color: 0.12,0.54,0.84,0.5
                font_size: 11
                pos_hint: {'center_x': 0.5, 'center_y': 0.4}
            MDTextButton:
                text: "aqui"
                custom_color: 0.12,0.54,0.84,0.9
                pos_hint: {'center_x': 0.5, 'center_y': 0.4}
            MDRoundFlatIconButton:
                text: '                        Entrar'
                icon: 'account-check'
                size_hint_x: .9
                pos_hint: {'center_x': 0.5, 'center_y': 0.3}
                on_release: app.root.current = 'WindowQuiz'
            MDFillRoundFlatIconButton:
                text: 'cadastrar'
                icon: 'account-plus'
                size_hint_x: .9
                md_bg_color: app.theme_cls.primary_color
                pos_hint: {'center_x': 0.5, 'center_y': 0.2}
                on_release: app.root.current = 'WindowOfCadastro'
<WindowOfCadastro>:
    BoxLayout:
        orientation: "vertical"
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
        size: root.width, root.height + 300
        pos_hint: {'center_x': 0.5, 'center_y': .5}
        MDToolbar:
            id: toolbar
            pos_hint: {"top": 1}
            elevation: 10
            spacing: 10
            title: "Cadastro"
            Image:
                source: 'C:/Users/PC/Documents/ProjetoHeelo/Screens/fotos/inscricao.png'
        ScrollView:
            BoxLayout:
                id: "BoxQuiz"
            	orientation: "vertical"
                spacing: 10
                padding: "20dp"
            	size_hint: None, None
                size: root.width - 50, root.height + 400
                pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Email
                    hint_text: "E-mail"
                    helper_text: ""
                    max_text_length: 100
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Nome
                    hint_text: "Nome Completo"
                    helper_text: ""
                    max_text_length: 100
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Senha
                    hint_text: "Senha"
                    helper_text: ""
                    password: True
                    max_text_length: 45
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: ComfirmSenha
                    hint_text: "Cofirmação de Senha"
                    helper_text: ""
                    password: True
                    max_text_length: 45
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDLabel:
                    text: "Sexo"
                    theme_text_color: "Custom"
                    text_color: 0, 0, 1, 1
                    size_hint: None, None
                    size: 300, 10
                BoxLayout:
                    orientation: "horizontal"
                    size_hint: None, None
                    size: 300, 30
                    Check:
                        id: Homem
                    MDLabel:
                        text: "Homem"
                        required: True
                        pos_hint: {'center_x': 0.5, 'center_y': .5}
                    Check:
                        id: Mulher
                    MDLabel:
                        text: "Mulher"
                        pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: DtNascimento
                    hint_text: "Data de Nascimento"
                    helper_text: "dia/mês/ano"
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Telefone
                    hint_text: "Telefone"
                    helper_text: "+DDI (DDD) XXXXX-XXXX"
                    helper_text_mode: "on_focus"
                    max_text_length: 15
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Categoria
                    hint_text: "Categoria"
                    helper_text: "A = 'Aluno', S = 'Servidor', T = 'Terceirizado'"
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    max_text_length: 1
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Perfil
                    hint_text: "Perfil"
                    helper_text: ""
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    max_text_length: 1
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Compus
                    hint_text: "Campus"
                    helper_text: ""
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Curso
                    hint_text: "Curso"
                    helper_text: ""
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Turma
                    hint_text: "Turma"
                    helper_text: ""
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Turno
                    hint_text: "Turno"
                    helper_text: ""
                    max_text_length: 1
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Data
                    hint_text: "Data"
                    helper_text: "dia/mês/ano"
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: Ativo
                    hint_text: "Ativo"
                    helper_text: ""
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    max_text_length: 1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
                MDTextField:
                    id: CPF
                    hint_text: "CPF"
                    helper_text: ""
                    password: True
                    max_text_length: 11
                    helper_text_mode: "on_focus"
                    color_mode: "custom"
                    fill_color: 0,0,0,1
                    required: True
                    pos_hint: {'center_x': 0.5, 'center_y': .5}
        BoxLayout:
            orientation: "vertical"
            size_hint: None, None
            size: 50, 50
            padding: "10dp"
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
            MDRoundFlatButton:
                text: 'Cadastrar'
                size_hint: None, None
                size: 200, 40
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                on_release: root.VerificationAnswersForm()

